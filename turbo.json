{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "www/**"]
    },
    "start:hellodash": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "www/**"]
    },
    "start:components": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "www/**"]
    },
    "start:dashbook": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "www/**"]
    }
  }
}
