{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "www/**"]
    },
    "build:components": {
      "dependsOn": ["^build:utils"],
      "outputs": ["dist/**", "www/**"]
    },
    "build:dev": {
      "dependsOn": ["^build:dev"],
      "outputs": ["dist/**", "www/**"]
    },
    "build:utils": {
      "outputs": ["dist/**", "www/**"]
    },
    "start:hellodash": {
      "dependsOn": ["^build:components"]
    },
    "start:components": {
      "dependsOn": ["^build:utils"]
    },
    "start:components:forge": {}
  }
}
