{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "www/**"]
    },
    "start:hellodash": {
      "dependsOn": ["^start:hellodash"],
      "outputs": ["dist/**", "www/**"]
    },
    "start:hellodash-react": {
      "dependsOn": ["^start:hellodash"],
      "outputs": ["dist/**", "www/**"]
    },
    "start:components": {
      "dependsOn": ["^start:components"],
      "outputs": ["dist/**", "www/**"]
    },
    "start:components:forge": {
      "dependsOn": ["^start:components:forge"],
      "outputs": ["dist/**", "www/**"]
    }
  }
}
