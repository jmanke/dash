{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "www/**"]
    },
    "build:dev": {
      "dependsOn": ["^build:dev"],
      "outputs": ["dist/**", "www/**"]
    },
    "start:hellodash": {
      "dependsOn": ["^build:dev", "^start:hellodash"],
      "outputs": ["dist/**", "www/**"]
    },
    "start:components": {
      "dependsOn": ["^build:dev", "^start:components"],
      "outputs": ["dist/**", "www/**"]
    },
    "start:components:forge": {
      "dependsOn": ["^start:components:forge"],
      "outputs": ["dist/**", "www/**"]
    }
  }
}
