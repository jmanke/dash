{
  "name": "@didyoumeantoast/hellodash",
  "private": true,
  "version": "1.0.0",
  "description": "Hellodash is a modern note taking web-app built with StencilJS",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "start:hellodash": "pnpm start",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "generate-icons": "cd ./bootstrap-icons && node extract-paths.js"
  },
  "devDependencies": {
    "@stencil/core": "^3.0.0",
    "@stencil/postcss": "^2.1.0",
    "@stencil/store": "^1.4.1",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.177",
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.9",
    "@types/uuid": "^8.3.3",
    "autoprefixer": "^10.3.5",
    "jest": "^27.2.5",
    "jest-cli": "^27.2.5",
    "postcss": "8.4.6",
    "postcss-extend-rule": "^4.0.0",
    "postcss-nested": "^5.0.6",
    "postcss-replace": "^1.1.3",
    "puppeteer": "^10.4.0",
    "rollup": "1.20.0",
    "rollup-plugin-dotenv": "^0.3.0"
  },
  "dependencies": {
    "@a11y/focus-trap": "^1.0.5",
