    "@types/qs": "^6.9.7",
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "luxon": "^2.4.0",
    "qs": "^6.10.3",
    "serve": "^13.0.4",
    "tinymce": "^5.10.2",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "18.x"
  },
  "volta": {
    "node": "18.12.1"
